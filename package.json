{
  "name": "react-rainbow-components",
  "version": "1.2.0-canary.2",
  "description": "React Rainbow Components",
  "keywords": [
    "react",
    "components",
    "library",
    "rainbow",
    "building-blocks",
    "react",
    "examples"
  ],
  "scripts": {
    "start:library": "./node_modules/.bin/styleguidist server",
    "watch:css": "node-sass-chokidar src/components/ -o src/components/ --watch --recursive",
    "start": "run-p start:library watch:css",
    "build:library": "./node_modules/.bin/styleguidist build",
    "build:components": "npx babel ./src --out-dir ./",
    "build:index": "npx babel index.js --out-dir components",
    "build:css": "node-sass-chokidar src/components/ -o ./components/",
    "build:fonts": "npx babel src/components/Application/fonts --out-dir components/Application/fonts --copy-files",
    "build": "run-p build:components build:index build:css build:fonts",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rm -Rf components && rm -Rf libs",
    "lint:js": "eslint src",
    "lint:scss": "stylelint src",
    "lint": "yarn lint:js && yarn lint:scss",
    "test": "jest ./src --verbose",
    "test:integration": "wdio",
    "generate:pageObjectDocs": "jsdoc -c jsdoc.conf.js",
    "deploy:library": "yarn build:library && yarn generate:pageObjectDocs && firebase deploy",
    "format": "prettier --write '{src,examples, library, tutorials}/**/*.{js,json,md}'"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "chart.js": "^2.7.3",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-async-script-loader": "^0.3.0",
    "react-dom": "^16.4.2"
  },
  "main": "components/index.js",
  "files": [
    "components",
    "propTypes",
    "libs"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/90milesbridge/react-rainbow.git"
  },
  "authors": [
    "Reinier Guerra",
    "Leandro Torres"
  ],
  "license": "MIT",
  "jest": {
    "collectCoverage": true,
    "testURL": "http://localhost/",
    "testPathIgnorePatterns": [
      "./examples"
    ],
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "moduleNameMapper": {
      "\\.(css|less|svg)$": "<rootDir>/__mocks__/assetsMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.1.0-11",
    "@fortawesome/free-regular-svg-icons": "^5.1.0-11",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "@wdio/cli": "^5.7.15",
    "@wdio/jasmine-framework": "^5.7.13",
    "@wdio/junit-reporter": "^5.7.16",
    "@wdio/local-runner": "^5.7.15",
    "@wdio/selenium-standalone-service": "^5.7.8",
    "@wdio/spec-reporter": "^5.7.13",
    "@wdio/sync": "^5.7.13",
    "assert": "^1.4.1",
    "babel-cli": "6.26.0",
    "babel-code-frame": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "clipboard-copy": "^2.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.26.0",
    "dotenv": "^6.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-webdriverio": "^1.0.1",
    "file-loader": "^1.1.11",
    "firebase-tools": "6.1.1",
    "glob": "^7.1.3",
    "husky": "^2.2.0",
    "jest": "^23.4.2",
    "jest-axe": "^3.1.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^12.0.0",
    "node-sass": "^4.9.3",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "pre-push": "^0.1.1",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.4.2",
    "react-ga": "^2.5.3",
    "react-prismic-cms": "^0.3.1",
    "react-redux": "^5.1.0",
    "react-styleguidist": "^7.2.1",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.0",
    "wdio-chromedriver-service": "^5.0.1",
    "webdriverio": "^5.7.15",
    "webpack": "^4.16.5"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
